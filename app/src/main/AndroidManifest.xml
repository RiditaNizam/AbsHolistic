<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.absholistic">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Food.FoodActivity" />
        <activity android:name=".Health.HealthActivity" />
        <activity android:name=".Body.BodyActivity" />
        <activity android:name=".Activity.ActivityActivity" />
        <activity android:name=".Calories.CaloriesActivity" />
        <activity android:name=".Diet.DietActivity" />
        <activity android:name=".Effort.EffortActivity" />
        <activity android:name=".Energy.EnergyActivity" />
        <activity android:name=".Fat.FatActivity" />
        <activity android:name=".Fitness.FitnessActivity" />
        <activity android:name=".Weight.WeightActivity" />
        <activity android:name=".Food.Supplements" />
        <activity android:name=".Food.MealIdeas"></activity>
    </application>

</manifest>